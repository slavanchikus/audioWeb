.container
  display: block
  margin-left: 20px
  position: relative
  >div
    cursor: pointer
    &:hover
      svg
        path
          fill: #ffa95e

.stick
  display: flex
  align-items: center
  justify-content: center
  width: 27px
  height: 115px
  position: absolute
  bottom: 46px
  left: -1px
  background: white
  border: 1px solid
  border-color: rgb(163, 162, 162, 0.53)
  border-radius: 5px
  >div
    width: 9px
    height: 100px
    position: relative;
    background: rgba(163, 162, 162, 0.33)
    >div
      width: 100%
      background: #ffa95e
      position: absolute
      bottom: 0
